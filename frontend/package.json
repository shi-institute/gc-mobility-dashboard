{
  "name": "gc-mobility-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run beforestart && vite",
    "build": "npm run beforestart && tsc -b && vite build",
    "build:gispreview": "NODE_ENV=development npm run build && npm run zipdist",
    "beforestart": "node beforeStart.js",
    "lint": "eslint .",
